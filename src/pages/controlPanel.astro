---
import Layout from "@layouts/Layout.astro";
import Container from "@components/container.astro";
import AdminPage from "@components/controlPanel/AdminPage.jsx";

const DATABASE_ID = import.meta.env.PUBLIC_DATABASE_ID;
const COLLECTION_ID = import.meta.env.PUBLIC_COLLECTION_ID;
const BUCKET_ID = import.meta.env.PUBLIC_BUCKET_ID;
---

<Layout title="">
  <Container>
    <AdminPage
      DATABASE_ID={DATABASE_ID}
      COLLECTION_ID={COLLECTION_ID}
      BUCKET_ID={BUCKET_ID}
      client:load
    />
  </Container>
</Layout>
